{
  "name": "tokens-to-css",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build:default && npm run build:sub",
    "build:default": "style-dictionary build --config sd.default.mjs",
    "build:sub": "style-dictionary build --config sd.subTheme01.mjs",
    "watch:default": "style-dictionary build --config sd.default.mjs --watch",
    "watch:sub": "style-dictionary build --config sd.subTheme01.mjs --watch",
    "clean": "rimraf dist .tmp || rm -rf dist .tmp"
  },
  "devDependencies": {
    "style-dictionary": "^5.0.1",
    "@tokens-studio/sd-transforms": "^2.0.1",
    "rimraf": "^6.0.1"
  }
}
